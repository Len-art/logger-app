{
  "name": "boiler",
  "version": "1.0.0",
  "description": "A boilerplate to start a React based project from.",
  "main": "index.js",
  "scripts": {
    "dev": "NEXT_ENV=staging next",
    "build": "next build",
    "start": "next start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "./node_modules/.bin/eslint"
  },
  "dependencies": {
    "next": "^7.0.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "classnames": "^2.2.6",
    "mobx": "^5.1.1",
    "mobx-react": "^5.2.8",
    "mobx-state-tree": "^3.3.0",
    "axios": "^0.18.0",
    "styled-jsx-plugin-stylelint": "^0.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/easwee/boiler.git"
  },
  "keywords": [
    "boilerplate",
    "react"
  ],
  "author": "easwee",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/easwee/boiler/issues"
  },
  "homepage": "https://github.com/easwee/boiler#readme",
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-module-resolver": "2.6.2",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint --fix -s",
      "git add"
    ]
  }
}
